<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>

<!-- Message Block -->
<%- messages() %>

<% if (errors) { %>
  <ul class="bad-notice">
  <% errors.array().forEach(error => { %>
      <li><%= error.msg %></li>
  <%  }) %>
  </ul>
<% } %>

<p>You're logged in!</p>